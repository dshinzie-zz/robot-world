<div class="wrapper-task">
  <form action="/robots" method="post" class="task-new">
    <p class="enter-task">Add a new robot</p>

    <div class="wrapper-edit">
      <div class='form-inline'>
        <div class='form-group'>
          <label for='robot-name'>Name:</label>
          <input type="text" name='robot[name]' id='robot-name' class='form-control enter-info' placeholder="name"/>
        </div>
      </div><br/>

      <div class='form-inline'>
        <div class='form-group'>
          <label for='robot-city'>City:</label>
          <input type="text" name='robot[city]' id='robot-city' class='form-control enter-info' placeholder="city"/>
        </div>
      </div><br/>

      <div class='form-inline'>
        <div class='form-group'>
          <label for='robot-state'>State:</label>
          <input type="text" name='robot[state]' id='robot-state' class='form-control enter-info' placeholder="state"/>
        </div>
      </div><br/>

      <div class='form-inline'>
        <div class='form-group'>
          <label for='robot-dept'>Department:</label>
          <input type="text" name='robot[department]' id='robot-dept' class='form-control enter-info' placeholder="department"/>
        </div>
      </div><br/>
      </div>

    <input type='submit' value='Submit' class='enter-submit'/>
  </form>
  <a class="back" href="/"><<</a>
</div>
